{
  "version": 2,
  "builds": [
    {
      "src": "dnd-client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "dnd-server/server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}